div(id='contact-container')
	#contact(ng-app='emailApp')
		#email-form(ng-controller='sendEmail')
			p#message1 Let's grab coffee.
			p#message2 Who knows where it may lead?
			input(placeholder='Name' ng-model='details.name')
			input(type='email' placeholder='Email' ng-model='details.email')
			textarea(rows='7' placeholder='Message' ng-model='details.msg')
			p#send(ng-click='sendEmailF($scope)') SEND
			#error Whoops